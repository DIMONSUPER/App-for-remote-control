<?xml version="1.0" encoding="utf-8" ?>
<tabs:BaseTabContentPage
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:tabs="clr-namespace:SmartMirror.Views.Tabs.Pages"
    xmlns:views="clr-namespace:SmartMirror.Views.Tabs.Views"
    xmlns:controls="clr-namespace:SmartMirror.Controls"
    xmlns:vm="clr-namespace:SmartMirror.ViewModels.Tabs.Pages"
    xmlns:extensions="clr-namespace:SmartMirror.Extensions"
    Title="{extensions:Translate Notifications}"
    x:Class="SmartMirror.Views.Tabs.Pages.NotificationsPage"
    x:DataType="vm:NotificationsPageViewModel">

    <controls:LazyContentView x:Name="lazyView">
        <DataTemplate>

            <views:NotificationsPageView/>

        </DataTemplate>
    </controls:LazyContentView>

</tabs:BaseTabContentPage>
